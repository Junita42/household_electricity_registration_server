from sqlalchemy import Column, Integer, String, Numeric, ForeignKey
from base_model import Base
from sqlalchemy.orm import relationship

class Heat_Pump(Base):
    __tablename__ = 'Heat_Pump'
    
    email = Column(String(256), ForeignKey('Household.email'), primary_key=True)
    air_handler_seq_num = Column(Integer, primary_key=True)
    SEER = Column(Numeric, nullable=False)
    HSPF = Column(Numeric, nullable=False)

    household = relationship("Household", back_populates="Heat_Pump")
